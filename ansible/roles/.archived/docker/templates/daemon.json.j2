{
  "log-driver": "{{ docker_daemon_config['log-driver'] | default('json-file') }}",
  "log-opts": {
    {% for key, value in docker_daemon_config['log-opts'].items() %}
    "{{ key }}": "{{ value }}"{% if not loop.last %},{% endif %}
    {% endfor %}
  },
  "storage-driver": "{{ docker_daemon_config['storage-driver'] | default('overlay2') }}",
  "live-restore": {{ docker_daemon_config['live-restore'] | default(true) | lower }},
  "userland-proxy": {{ docker_daemon_config['userland-proxy'] | default(false) | lower }},
  "no-new-privileges": {{ docker_daemon_config['no-new-privileges'] | default(true) | lower }},
  "seccomp-profile": "{{ docker_daemon_config['seccomp-profile'] | default('/etc/docker/seccomp.json') }}",
  "icc": {{ docker_daemon_config['icc'] | default(false) | lower }},
  "userns-remap": "{{ docker_daemon_config['userns-remap'] | default('default') }}",
  "default-ulimits": {
    {% for ulimit_name, ulimit_config in docker_daemon_config['default-ulimits'].items() %}
    "{{ ulimit_name }}": {
      "Name": "{{ ulimit_config.name }}",
      "Soft": {{ ulimit_config.soft }},
      "Hard": {{ ulimit_config.hard }}
    }{% if not loop.last %},{% endif %}
    {% endfor %}
  },
  "hosts": [
    "fd://",
    "unix:///var/run/docker.sock"
  ],
  "tls": false,
  "debug": false,
  "experimental": false,
  "ip-forward": false,
  "ip-masq": false,
  "iptables": true,
  "ipv6": false,
  "bridge": "docker0",
  "fixed-cidr": "172.17.0.0/16"
}